{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "bacon/bacon-qr-code": "~1.0.3",
        "camroncade/timezone": "0.1",
        "consoletvs/charts": "6.*",
        "darkaonline/l5-swagger": "5.8.*",
        "dubstepmad/formbuilder": "dev-master",
        "fideloper/proxy": "^4.0",
        "genert/bbcode": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "invisnik/laravel-steam-auth": "^4.0",
        "itsjavi/bootstrap-colorpicker": "^3.1",
        "jenssegers/agent": "^2.6",
        "kordy/ticketit": "0.*",
        "laminas/laminas-diactoros": "^2.2",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.3",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.8",
        "more-cores/laravel-restcord": "2.*",
        "nesbot/carbon": "^2.21",
        "pragmarx/google2fa-laravel": "^1.0",
        "rap2hpoutre/laravel-log-viewer": "^1.3",
        "socialiteproviders/discord": "^2.0",
        "socialiteproviders/mixer": "^1.0",
        "socialiteproviders/twitch": "^5.0",
        "socialiteproviders/youtube": "^3.0",
        "spatie/laravel-cookie-consent": "^2.11",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^2.37"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
